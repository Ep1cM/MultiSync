    </svg>

    <main class="d-flex flex-nowrap">
        <div class="b-example-divider b-example-vr"></div>

        <div class="d-flex flex-column flex-shrink-0 bg-body-tertiary" style="width: 4.5rem;">
            <a href="/" class="d-block p-3 link-body-emphasis text-decoration-none" title="Icon-only" data-bs-toggle="tooltip" data-bs-placement="right">
                <svg class="bi pe-none" width="40" height="32"><use xlink:href="#bootstrap" /></svg>
                <span class="visually-hidden">Icon-only</span>
            </a>
            <ul class="nav nav-pills nav-flush flex-column mb-auto text-center">
                <li class="nav-item">
                    <a href="/" id="home" class="nav-link py-3 border-bottom rounded-0" aria-current="page" title="Home" data-bs-toggle="tooltip" data-bs-placement="right">
                        <svg class="bi pe-none" width="24" height="24" role="img" aria-label="Home"><use xlink:href="#home" /></svg>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "MS")" id="mssql" class="nav-link py-3 border-bottom rounded-0" title="MSSql" data-bs-toggle="tooltip" data-bs-placement="right">
                        <svg class="bi pe-none" width="24" height="24" role="img" aria-label="Dashboard"><use xlink:href="#mssql" /></svg>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "MongoDB")" id="mongo" class="nav-link py-3 border-bottom rounded-0" title="MongoDB" data-bs-toggle="tooltip" data-bs-placement="right">
                        <svg class="bi pe-none" width="24" height="24" role="img" aria-label="Orders"><use xlink:href="#mongo" /></svg>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "XML")" id="xml" class="nav-link py-3 border-bottom rounded-0" title="XML" data-bs-toggle="tooltip" data-bs-placement="right">
                        <svg class="bi pe-none" width="24" height="24" role="img" aria-label="Products"><use xlink:href="#xml" /></svg>
                    </a>
                </li>
                <li>
                    <a href="@Url.Action("Index", "Sync")" id="sync" class="nav-link py-3 border-bottom rounded-0" title="Synchronization" data-bs-toggle="tooltip" data-bs-placement="right">
                        <svg class="bi pe-none" width="24" height="24" role="img" aria-label="Customers"><use xlink:href="#sync" /></svg>
                    </a>
                </li>
                <partial name="_LoginPartial" />
            </ul>
        </div>
        <!-- Page content -->
        <div class="container-fluid">
            <div class="container">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
    </main>

    <!-- JavaScript libraries -->
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>